{
    "Logging": {
        "LogFileName": "CamelotLog-{Date}.txt",
        "LimitBytes": 10485760,
        "DefaultLogLevel": "Warning",
        "MicrosoftLogLevel": "Warning"
    },
    "About": {
        "RepositoryUrl": "https://github.com/IngvarX/Camelot",
        "Maintainers": ["Ingvar"]
    },
    "DataAccess": {
        "DatabaseName": "Camelot.db",
        "ConnectionString": "FileName={0};Connection=Shared"
    },
    "FileSystemWatcher": {
        "RefreshIntervalMs": 1000
    },
    "ImagePreview": {
        "SupportedFormats": ["bmp", "jpg", "png", "gif"]
    },
    "FilePanel": {
        "SaveTimeoutMs": 500
    },
    "SearchPanel": {
        "TimeoutMs": 250
    },
    "Drives": {
        "DrivesListRefreshIntervalMs": 1000
    },
    "UnmountedDrives": {
        "IsEnabled": true
    },
    "Archive": {
        "ExtensionToArchiveTypeDictionary": {
            "tar": "Tar",
            "zip": "Zip",
            "tar.gz": "TarGz",
            "tgz": "TarGz",
            "bz2": "Bz2",
            "tar.bz2": "TarBz2",
            "xz": "TarXz",
            "tar.xz": "TarXz",
            "7z": "SevenZip",
            "7zip": "SevenZip",
            "gz": "GZip",
            "lz": "TarLz",
            "tar.lz": "TarLz"
        }
    }
}